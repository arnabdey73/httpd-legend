	# legacy_url rewrite
	RewriteCond ${legacy_zebra:${lc:$2}} >""
	RewriteRule ^/([a-z][a-z]/[a-z][a-z])/(.*)$  https://%{HTTP_HOST}/$1/${legacy_zebra:${lc:$2}|${lc:$2}} [R,L]

	# locale rewrite
        RewriteCond ${locale:$1} >""
	RewriteRule ^/([A-Z]+-[A-Z]+)(/.*)?$         https://%{HTTP_HOST}/${locale:$1}$2	[R,L]

        # locale rewrite
        RewriteCond ${locale_remap:$1} >""
        RewriteRule ^/([a-z][a-z]/[a-z][a-z])(/?.*)$  https://%{HTTP_HOST}/${locale_remap:$1}$2     [R,L]

	# sub_path rewrite
	RewriteCond ${sub_path:$2} >""
	RewriteRule ^/([a-z][a-z]/[a-z][a-z])/(.*)$   https://%{HTTP_HOST}/$1${sub_path:$2|$2}	 [R,L]
