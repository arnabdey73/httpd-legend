# Handle request that starts with /content/zebra1
   RewriteCond     %{HTTP_HOST}   ^(stage|qa)-www.zebra.com$  [OR]
   RewriteCond     %{HTTPS_HOST}  ^(stage|qa)-www.zebra.com$
   RewriteCond     %{REQUEST_URI} ^/content/zebra1*
   RewriteCond     %{REQUEST_URI} .*.html
   RewriteRule     ^/content/zebra1/(.*)$ /$1 [R,QSA]

# Handle requests that does not contain /content/zebra1/
   RewriteCond     %{HTTP_HOST}   ^(stage|qa)-www.zebra.com$  [OR]
   RewriteCond     %{HTTPS_HOST}  ^(stage|qa)-www.zebra.com$
   RewriteCond     %{REQUEST_URI} !^/content/zebra1/
   RewriteCond     %{REQUEST_URI} !^/content/dam
   RewriteCond     %{REQUEST_URI} !^/libs
   RewriteCond     %{REQUEST_URI} !^/etc
   RewriteCond     %{REQUEST_URI} !^/apps
   RewriteCond     %{REQUEST_URI} .*.(html|json)
   RewriteCond     %{REQUEST_URI} !^/$
   RewriteRule     ^/(.*).(html|json)$ /content/zebra1/$1.$2 [PT,L,QSA]


